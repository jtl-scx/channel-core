{
    "name": "jtl/channel-lib",
    "type": "library",
    "description": "SCX Channel Library",
    "require": {
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "jtl/channel-api-client": "0.0.6",
        "jtl/nachricht": "^0.2.0",
        "jtl/scx-api-client": "^0.0.1",
        "monolog/monolog": "^2.0",
        "myclabs/php-enum": "^1.7",
        "php-amqplib/php-amqplib": "^2.10",
        "psr/log": "^1.1",
        "symfony/config": "^4.3",
        "symfony/console": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/proxy-manager-bridge": "^4.3",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpstan/phpstan": "^0.11.15",
        "twig/twig": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "JTL\\SCX\\Lib\\Channel\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@gitlab.jtl-software.com:ea/scx/channel-api-client.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.jtl-software.com:ea/scx/scx-api-client.git"
        }
    ],
    "scripts": {
        "phpcs": [
            "vendor/bin/php-cs-fixer fix --config=./.php_cs.dist"
        ],
        "phpcs-dry": [
            "vendor/bin/php-cs-fixer fix --config=./.php_cs.dist --dry-run"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
