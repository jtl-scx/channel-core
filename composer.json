{
    "name": "jtl-scx/channel-core",
    "type": "library",
    "description": "SCX Channel Library",
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "jtl-scx/api-base-client": "^1.0",
        "jtl-scx/channel-api-client": "^0.16",
        "jtl/nachricht": "^0.7",
        "monolog/monolog": "^2.0",
        "myclabs/php-enum": "^1.7",
        "php-amqplib/php-amqplib": "^2.10",
        "psr/log": "^1.1",
        "symfony/config": "^4.3",
        "symfony/console": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/proxy-manager-bridge": "^4.3",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpstan/phpstan": "^0.11.15",
        "phpunit/phpunit": "^9.0",
        "symfony/dotenv": "^5.0",
        "twig/twig": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "JTL\\SCX\\Lib\\Channel\\": "src/"
        }
    },
    "scripts": {
        "phpcs": [
            "vendor/bin/php-cs-fixer fix --config=./.php_cs.dist"
        ],
        "phpcs-dry": [
            "vendor/bin/php-cs-fixer fix --config=./.php_cs.dist --dry-run"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
